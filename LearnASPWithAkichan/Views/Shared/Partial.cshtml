      aria-hidden="true">
      <div class="modal-dialog modal-sm modal-notify modal-danger" role="document">
        <!--Content-->
        <div class="modal-content text-center">
          <!--Header-->
          <div class="modal-header d-flex justify-content-center">
            <p class="heading">Xác nhận xoá kỳ này? </p>
          </div>

          <!--Body-->
          <div class="modal-body">

            <i class="fa fa-times fa-4x animated rotateIn" aria-hidden="true"></i>

          </div>

          <!--Footer-->
          <div class="modal-footer" style="display:flex !important; justify-content: center !important;">
            @*<a href="" class="btn btn-outline-danger">Xác nhận</a>

             <button type="button" class="btn btn-default" data-dismiss="modal">Thôi khỏi</button>
                <form asp-action="xoaKyDangKy" class="d-inline">
                      <button type="submit" class="btn btn-warning btn-xs"><i class="fas fa-trash"></i> Xoá</button>

                </form>
          </div>
        </div>
        <!--/.Content-->
      </div>
    </div>
